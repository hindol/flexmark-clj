  [:text {} "."]]
 [:paragraph
  {}
  [:text
   {}
   "To avoid exposing your private key, create signed transactions offline and send the signed transaction data using "]
  [:code {} [:text {} "eth_sendRawTransaction"]]
  [:text {} "."]]
