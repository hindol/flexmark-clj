   [:paragraph
    {}
    [:text
     {}
     "DATA, 32 Bytes - Hash of the current block header (pow-hash)."]]]
  [:bullet-list-item
   {}
   [:paragraph
    {}
    [:text {} "DATA, 32 Bytes - The seed hash used for the DAG."]]]
  [:bullet-list-item
   {}
   [:paragraph
    {}
    [:text
     {}
     "DATA, 32 Bytes - The target boundary condition to be met; 2^256 / difficulty."]]]]
