   {}
   [:text
    {}
    "{\"jsonrpc\":\"2.0\",\"method\":\"priv_deletePrivacyGroup\",\"params\":[\"ewuTVoc5nlvWMwTFdRRK/wvV0dcyQo/Pauvx5bNEbTk=\"],\"id\":1}\n"]]
